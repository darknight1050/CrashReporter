{
    "version": "0.1.0",
    "sharedDir": "shared",
    "dependenciesDir": "extern",
    "info": {
        "name": "CrashReporter",
        "id": "crashreporter",
        "version": "0.0.1",
        "url": "https://github.com/darknight1050/CrashReporter",
        "additionalData": {
