        {
            "id": "bsml",
            "versionRange": "^0.4.39",
            "additionalData": {}
        },
        {
            "id": "config-utils",
            "versionRange": "^1.4.2",
            "additionalData": {}
        },
        {
            "id": "scotland2",
            "versionRange": "^0.1.4",
