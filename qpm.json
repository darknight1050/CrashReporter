{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "CrashReporter",
    "id": "crashreporter",
    "version": "0.0.1",
    "url": "https://github.com/darknight1050/CrashReporter",
    "additionalData": {
      "overrideSoName": "libcrashreporter.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "log": [
        "pwsh ./scripts/log.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "qpm qmod manifest",
        "pwsh ./scripts/createqmod.ps1 CrashReporter -clean"
      ]
    },
    "ndk": "^27.3.13750724",
    "qmodIncludeDirs": [
      "build",
      "extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "CrashReporter.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "4008.*",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.55",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^2.0.3",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "libcurl",
      "versionRange": "=8.5.0",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    }
  ]
}
